<div id="dst">
  
  <ul>
    <li id="header">
      <h1><span class="highlight"><%= 'Demographics'.pluralize(1) %></span> for the <span class="current"><%= @version.creative %></span> version of the <%= link_to @version.campaign.name, brand_campaign_path(@version.campaign.brand, @version.campaign) %> campaign that belongs to the <%= link_to @version.campaign.brand.name, brand_path(@version.campaign.brand), :style => 'color:#' + @version.campaign.brand.color %> <%= link_to 'brand.', brands_path %></h1>
    </li>
    <li id="new">
      <%= link_to 'New Demographic', new_brand_campaign_version_demographic_path(@version.campaign.brand, @version.campaign, @version) %>
    </li>
    <% @version.demographics.each do |demographic| %>
      <li><%= demographic.audience.code %></li>
    <% end %>
  </ul>
  
</div>